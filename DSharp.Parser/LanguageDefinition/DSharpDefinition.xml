<?xml version="1.0" encoding="utf-8" ?>
<Language xmlns="http://tempuri.org/GenericLanguageDefinition.xsd"
					Name="D#"
					RootElement="Module">
	<Element Name="Module">
		<Notes>
			The module's entry node
		</Notes>
		<SubElement Ref="DeclDefs"/>
	</Element>

	<Element Id="DeclDefs">
		<SubElement Ref="NamespaceDefinition" IsOptional="true" OnceOnly="true"/>
		<SubElement Ref="DeclDef"/>
	</Element>

	<DefinedSequence Name="NamespaceDefinition">
		<Notes>
			namespace A.B.C;
		</Notes>
		<Token Text="namespace"/>
		<SubElement Ref="DottedIdentifierChain" OnceOnly="true"/>
		<Token Text=";"/>
	</DefinedSequence>

	<DefinedSequence Id="DottedIdentifierChain">
		<Notes>
			A.B.C.D
		</Notes>
		<Token Ref="Identifier" OnceOnly="true"/>
		<SubElement Ref="DotIdentifier" IsOptional="true"/>
	</DefinedSequence>

	<DefinedSequence Id="DotIdentifier">
		<Notes>
			.B
		</Notes>
		<Token Text="."/>
		<Token Ref="Identifier" OnceOnly="true"/>
	</DefinedSequence>



	<ElementChoice Id="DeclDef">
		
	</ElementChoice>

</Language>